{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./*"]
        },
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "nativewind-env.d.ts",
        "components/button/.test.tsx"
    ]
}
